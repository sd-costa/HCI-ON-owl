#################################################################

###  https://nemo.inf.ufes.br/HCIO/hcio#bringsAbout
hcio:bringsAbout rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#causedBy
hcio:causedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#causes
hcio:causes rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:TransitiveProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#composedOf
hcio:composedOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:SymmetricProperty ,
                         owl:ReflexiveProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#connects
hcio:connects rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#constitutedOf
hcio:constitutedOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#directlyCauses
hcio:directlyCauses rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ,
                             owl:ReflexiveProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#executionOf
hcio:executionOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#handles
hcio:handles rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#has
hcio:has rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#includes
hcio:includes rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#inheresIn
hcio:inheresIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#interprets
hcio:interprets rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#isDoneUsing
hcio:isDoneUsing rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#materializationOf
hcio:materializationOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#mayEvaluateAchievementOf
hcio:mayEvaluateAchievementOf rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdf:type owl:FunctionalProperty .


###  https://nemo.inf.ufes.br/HCIO/hcio#participationOf
hcio:participationOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
